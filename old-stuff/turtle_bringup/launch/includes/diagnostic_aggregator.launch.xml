<launch>

  <arg name="start_with_diagnostic" default="true"/>

  <!-- diagnostic Node -->
  <node if="$(arg start_with_diagnostic)" pkg="diagnostic_aggregator" type="aggregator_node" name="diagnostic_aggregator" >
    <rosparam command="load" file="$(find turtle_bringup)/param/defaults/diagnostics.yaml" />
  </node>

</launch>
