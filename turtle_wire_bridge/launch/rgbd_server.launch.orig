<?xml version="1.0"?>

<launch>

    <node name="rgbd_server" pkg="rgbd" type="rgbd_server" output="screen">
    
        #<remap from="cam_info" to="PUT THE CAM INFO TOPIC HERE, e.g., /amigo/top_kinect/rgb/camera_info"/>
        <remap from="cam_info" to="/camera/rgb/camera_info"/>
        
        #<remap from="rgb_image" to="PUT THE RGB TOPIC HERE, e.g., /amigo/top_kinect/rgb/image_color"/>
        <remap from="rgb_image" to="/camera/rgb/image_raw"/>
        
		#<remap from="depth_image" to="PUT THE DEPTH TOPIC HERE, e.g., /amigo/top_kinect/depth_registered/hw_registered/image_rect_raw"/>
		<remap from="depth_image" to="/camera/depth/image_raw"/>
		
        <remap from="output" to="/kinect/rgbd"/>
    </node>

</launch>
